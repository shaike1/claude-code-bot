#!/usr/bin/expect -f

set timeout 15
log_user 1

# First script: Just complete the theme setup
spawn claude --help

expect {
    "Choose the text style" {
        puts "\n=== THEME SETUP ==="
        send "1\r"
        expect {
            "Usage:" {
                puts "\n=== HELP SHOWN - SETUP COMPLETE ==="
            }
            timeout {
                puts "\n=== TIMEOUT AFTER THEME ==="
            }
        }
    }
    "Usage:" {
        puts "\n=== HELP SHOWN DIRECTLY - NO SETUP NEEDED ==="
    }
    timeout {
        puts "\n=== TIMEOUT ==="
    }
}

puts "\n=== SETUP SCRIPT COMPLETE ==="